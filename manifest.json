{
	"name": "Intip.in's Tab Manager",
	"description": "Tab Manager that doesn't track you.",
	"version": "1.0.4",
	"manifest_version": 2,
	"options_page": "options.html",
	"icons": {
		"32": "img/icon_32.png",
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "img/icon_48.png",
		"default_title": "Save all tabs"
	},
	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage",
		"contextMenus"
	]
}
